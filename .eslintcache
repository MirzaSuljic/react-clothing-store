port './Checkout.css';\r\nimport Information from './views/Information/Information';\r\nimport Payment from './views/Payment/Payment';\r\nimport Confirm from './views/Confirm/Confirm';\r\nimport Thankyou from './views/Thankyou/Thankyou';\r\n\r\nclass Checkout extends Component {\r\n    constructor(props) {\r\n        super(props);\r\n        this.state = {\r\n            currentView: 'information',\r\n            paymentProvider: '',\r\n            cartItems: JSON.parse(localStorage.getItem('cart')) || [],\r\n            tax: 81,\r\n            // validation:{\r\n            //     required:true\r\n            // },\r\n            // valid: false,\r\n          email: '',\r\n          firstName: '',\r\n          lastName: '',\r\n          company : '',\r\n          address : '',\r\n          apartment : '',\r\n          postalCode : '',\r\n          city : '',\r\n          country : '',\r\n          phone : '',\r\n          };\r\n\r\n          this.goToNextStep = this.goToNextStep.bind(this);\r\n          this.getInformation = this.getInformation.bind(this);\r\n          this.getPaymentProvider = this.getPaymentProvider.bind(this);\r\n      }\r\n    //   checkValidity(value, rules){\r\n    //       let isValid = false;\r\n    //     if(rules.required){\r\n    //         isValid = value.trim() !== '';\r\n    //     }\r\n    //     return isValid;\r\n    //   }\r\n\r\n      getCurrentView() {\r\n          let view;\r\n          switch (this.state.currentView) {\r\n              case 'information':\r\n                    view = <Information goToNextStep={this.goToNextStep} getInformation={this.getInformation} />\r\n                    break;\r\n                case 'payment':\r\n                    view = <Payment goToNextStep={this.goToNextStep} getPaymentProvider={this.getPaymentProvider} />\r\n                    break;\r\n                case 'confirm':\r\n                    view = <Confirm goToNextStep={this.goToNextStep} info={this.state} />\r\n                    break;\r\n                case 'thankyou':\r\n                    view = <Thankyou />\r\n                    break;\r\n                default:\r\n          }\r\n\r\n          return view;\r\n      }\r\n\r\n      getPaymentProvider(provider) {\r\n          this.setState({\r\n              paymentProvider: provider\r\n          });\r\n      }\r\n\r\n      getInformation(info) {\r\n          this.setState({\r\n              email: info.email,\r\n              firstName: info.firstName,\r\n                lastName: info.lastName,\r\n                company : info.company,\r\n                address : info.address,\r\n                apartment : info.apartment,\r\n                postalCode : info.postalCode,\r\n                city : info.city,\r\n                country : info.country,\r\n                phone : info.phone,\r\n          });\r\n      }\r\n\r\n      goToNextStep(nextStep) {\r\n        this.setState({\r\n            currentView: nextStep\r\n        })\r\n      }\r\n\r\n    render(){\r\n        const subtotal = this.state.cartItems.reduce((total, item) => total += item.price * item.quantity,  0);\r\n        const currentView = this.getCurrentView();\r\n        return (\r\n            <div className=\"checkout\">\r\n                <div className=\"checkout-views\">{currentView}</div>\r\n                <div className=\"checkout-cart-info\">\r\n                    <h2>Cart</h2>\r\n                    <ul className=\"cart-items\">\r\n                        {this.state.cartItems.map((item, i) => {\r\n                            return (\r\n                                <li key={i}>\r\n                                    <div className=\"cart-item-image\" style={{backgroundImage: `url(${item.images[0]})`}}></div>\r\n                                    <div>\r\n                                        <h3>{item.title}</h3>\r\n                                        <p>${item.price} x {item.quantity}</p>\r\n                                    </div>\r\n                                </li>\r\n                            );\r\n                        })}\r\n                    </ul>\r\n                    <div className=\"checkout-totals\">\r\n                        <div>\r\n                            <div>Subtotal</div>\r\n                            <div>${subtotal}</div>\r\n                        </div>\r\n                        <div>\r\n                            <div>Taxes</div>\r\n                            <div>${this.state.tax}</div>\r\n                        </div>\r\n                    </div>\r\n                    <div className=\"checkout-total\">\r\n                        <div>Total</div>\r\n                        <div>${subtotal + this.state.tax}</div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        )\r\n\r\n    }\r\n}\r\n\r\nexport default Checkout;\r\n","C:\\Users\\DT User6\\OneDrive\\Documents\\Projekti\\React\\react-clothing-store\\src\\pages\\Details\\Details.js",[],"C:\\Users\\DT User6\\OneDrive\\Documents\\Projekti\\React\\react-clothing-store\\src\\pages\\Checkout\\views\\Information\\Information.js",[],"C:\\Users\\DT User6\\OneDrive\\Documents\\Projekti\\React\\react-clothing-store\\src\\pages\\Checkout\\views\\Confirm\\Confirm.js",[],"C:\\Users\\DT User6\\OneDrive\\Documents\\Projekti\\React\\react-clothing-store\\src\\pages\\Checkout\\views\\Thankyou\\Thankyou.js",["96"],"import React, { Component } from 'react'\r\nimport Logo from '../../../../assets/logo.svg';\r\nimport Arrow from '../../../../assets/arrow-right.svg';\r\nimport {Link} from 'react-router-dom';\r\nimport './Thankyou.css';\r\n\r\nclass Thankyou extends Component { \r\n    constructor(props) {\r\n        super(props);\r\n\r\n      }\r\n\r\n    render(){\r\n\r\n        return (\r\n                <div className=\"checkout-thankyou\">\r\n                    <div className=\"logo1\">\r\n                        <Link to=\"/\">\r\n                            <img src={Logo} alt=\"logo\"></img>\r\n                        </Link>\r\n                    </div>\r\n                    <div className=\"tab-links\">\r\n                        <div className=\"text-links mr-15\">Information</div>\r\n                        <div className=\"mr-15\"> <img src={Arrow} alt=\"Arrow\"></img></div>\r\n                        <div className=\"text-links mr-15\">Payment</div>\r\n                        <div className=\"mr-15\"> <img src={Arrow} alt=\"Arrow\"></img></div>\r\n                        <div className=\"text-links mr-15 tab-link-active\">Confirm</div>\r\n                    </div>\r\n                   <div>\r\n                        <h2>Thank you for your order!</h2>\r\n                        <p>The order will be processed within next 48 hours.</p>\r\n                   </div>\r\n                </div>\r\n        )\r\n\r\n    }\r\n}\r\n\r\nexport default Thankyou;\r\n","C:\\Users\\DT User6\\OneDrive\\Documents\\Projekti\\React\\react-clothing-store\\src\\pages\\Homepage\\Homepage.js",[],"C:\\Users\\DT User6\\OneDrive\\Documents\\Projekti\\React\\react-clothing-store\\src\\pages\\Registration\\Registration.js",[],"C:\\Users\\DT User6\\OneDrive\\Documents\\Projekti\\React\\react-clothing-store\\src\\pages\\Women\\Women.js",["97"],"import React, { Component } from 'react';\r\nimport {Link} from 'react-router-dom';\r\nimport './Women.css';\r\n\r\nclass Women extends Component {\r\n    constructor(props) {\r\n        super(props);\r\n    } \r\n    \r\n    galery={\r\n        images:[],\r\n        description: '',\r\n        price: '',\r\n    }\r\n\r\n    render(){\r\n        return (\r\n            <div> \r\n                <span className=\"title\">Women</span>\r\n            <div className=\"women\">\r\n           {this.props.apiData.api.filter(data => data.gender === 'w').map(women => {\r\n               return (\r\n                   <div key={women.id}>\r\n                       <Link to={`/detail/${women.id}`}>\r\n                    <div\r\n     